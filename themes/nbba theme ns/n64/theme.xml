<theme>
<formatVersion>3</formatVersion>
<include>./../theme.xml</include>

<!-- Nintendo 64 - 5th gen, September 29th 1996 -->
<!-- Normal: 069330 -->
<!-- Light : 63f991 -->
<!-- Dark  : 045c1f -->

<view name="system, basic, detailed">
	
	<image name="logo">
		<path>./../../nbba theme/n64/logo.svg</path>
	</image>
	
	<helpsystem name="help">
		<textColor>045c1f</textColor>
		<iconColor>045c1f</iconColor>
	</helpsystem>
	
</view>

<view name="system">
	
	<image name="background" extra="true">
		<path>./../../nbba theme/n64/bg.jpg</path>
	</image>
	
	<image name="gc" extra="true">
		<path>./../../nbba theme/n64/gc.png</path>
	</image>
	
	<text name="year" extra="true">
		<color>63f991</color>
		<text>5th gen, September 29th 1996</text>
	</text>
	
</view>

<view name="basic, detailed">
	
	<image name="background">
		<color>069330</color>
	</image>
	
	<text name="md_lbl_rating, md_lbl_releasedate, md_lbl_developer, md_lbl_publisher, md_lbl_genre, md_lbl_players, md_lbl_lastplayed, md_lbl_playcount, md_developer, md_publisher, md_genre, md_players, md_playcount, md_description">
		<color>63f991</color>
	</text>
		<rating name="md_rating">
		<filledPath>./../../nbba theme/n64/star_filled.svg</filledPath>
		<unfilledPath>./../../nbba theme/n64/star_unfilled.svg</unfilledPath>
	</rating>
	
	<datetime name="md_releasedate, md_lastplayed">
		<color>63f991</color>
	</datetime>
	
	<textlist name="gamelist">
		<selectorColor>63f99170</selectorColor>
		<selectedColor>045c1f</selectedColor>
		<primaryColor>63f99170</primaryColor>
		<secondaryColor>63f99170</secondaryColor>
	</textlist>
	
</view>

<view name="basic">
	
</view>

<view name="detailed">
	
</view>
	
</theme>